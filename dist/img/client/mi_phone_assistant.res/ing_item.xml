<?xml version="1.0" encoding="utf-8"?>
<Window>
  <ListHBoxElement height="60" childalign="left" >
    <VerticalLayout childalign="left" height="60">
		<Control name="line" bkimage="imgs\home_sp.png" padding="0,0,0,0" height="1" />
		<HorizontalLayout childalign="left" >
			<VerticalLayout  width="26"/>
				<Control name="pic" bkimage="" height="26" width="26"/>
			<VerticalLayout />
				
			<VerticalLayout width="600" >
				<Label name="name" align="left" height="20" textcolor="#444c58" padding="0,10,0,0"/>
				<Label name="status" textcolor="#8b94a5" height="18" padding="0,0,0,0" align="left" />
			</VerticalLayout >
				
			<VerticalLayout width="50" childalign="left" childvalign="center">
				<VerticalLayout />
				<Control name="before_pic" bkimage="imgs\icon_back_up_before.png" height="18" width="18"/>
				<GifAnim name="ing_pic" bkimage="imgs\icon_back_up_ing .gif" width="18" height="18"/>
				<Control name="end_pic" bkimage="imgs\icon_back_up_succ.png" height="18" width="18"/>
				<VerticalLayout />
			</VerticalLayout >
		</HorizontalLayout >
		<Control name="line_end" bkimage="imgs\home_sp.png" padding="0,0,0,0" height="1" visible="false"/>
	</VerticalLayout >
  </ListHBoxElement>
</Window>